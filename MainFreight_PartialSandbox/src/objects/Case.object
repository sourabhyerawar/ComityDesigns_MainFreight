    <validationRules>
        <fullName>AR_NonStandard_Account_Type_Validation</fullName>
        <active>false</active>
        <description>If Account Type = Non-Standard make sure Account&gt; Action Request Type = Non-Standard else Error</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Internal_Support&apos; 
&amp;&amp;
ISPICKVAL( Account.Action_Request_Type__c, &quot;Standard&quot; ) 
&amp;&amp;  
TEXT( Account_Type__c ) != &quot;Standard&quot;</errorConditionFormula>
        <errorDisplayField>Account_Type__c</errorDisplayField>
        <errorMessage>For Account Action Request Type = Standard, the Account Type Should be Standard.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AR_Standard_Account_Type_Validation</fullName>
        <active>false</active>
        <description>If Account Type = Standard make sure Account&gt; Action Request Type = Standard else Error.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Internal_Support&apos; 
&amp;&amp;  
ISPICKVAL( Account.Action_Request_Type__c, &quot;Non-Standard&quot; )
&amp;&amp;
(
 Account.Name != TEXT( Account_Type__c )
 ||
 ISPICKVAL( Account_Type__c, &quot;Standard&quot; )
)</errorConditionFormula>
        <errorDisplayField>Account_Type__c</errorDisplayField>
        <errorMessage>There is no Account Type and Non-standard Action Request category for this Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Activity_Type_should_be_closed</fullName>
        <active>true</active>
        <description>Activity Type should be closed as the case status is closed</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;Internal_Support&apos; &amp;&amp;    ISPICKVAL(Status, &apos;Closed&apos;)  &amp;&amp; 
NOT(ISPICKVAL(Activity_Type__c, &apos;Closed&apos;))</errorConditionFormula>
        <errorDisplayField>Activity_Type__c</errorDisplayField>
        <errorMessage>Activity Type should be closed as the case status is closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fields_Cannot_Be_Changed_On_Case_Closure</fullName>
        <active>true</active>
        <description>When the Action Request Case is Closed the fields:
Root Cause, Immediate Action, Corrective Action, Auditor Comments should be not editable.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Internal_Support&apos; 

&amp;&amp; 

NOT( ISCHANGED( IsClosed ) ) &amp;&amp; ISPICKVAL( Status, &apos;Closed&apos; ) &amp;&amp; ( ISCHANGED( Root_Cause__c ) || ISCHANGED( Immediate_Action__c ) || ISCHANGED( Corrective_Action__c ) || ISCHANGED( Auditor_Comments__c ) )</errorConditionFormula>
        <errorMessage>Root Cause, Immediate Action, Corrective Action, Auditor Comments cannot be changed once the Action Request is Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Closed_Activity_Type</fullName>
        <active>true</active>
        <description>Root Cause, Immediate Action, Corrective Action, Auditor Comments fields cannot be empty when When Activity Type is Closed.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Internal_Support&apos; 

&amp;&amp; 

ISPICKVAL( Activity_Type__c, &apos;Closed&apos; )

&amp;&amp;

( ISBLANK( Root_Cause__c ) || ISBLANK( Immediate_Action__c) || ISBLANK( Corrective_Action__c) || ISBLANK( Auditor_Comments__c ) )</errorConditionFormula>
        <errorMessage>Root Cause, Immediate Action, Corrective Action, Auditor Comments fields cannot be empty when Activity Type is Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Implementation_Activity_Type</fullName>
        <active>true</active>
        <description>When the Activity Type = Implementation, Root Cause, Immediate Action, Corrective Action cannot be empty.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Internal_Support&apos;

&amp;&amp;

ISPICKVAL( Activity_Type__c, &apos;Implementation&apos; ) 

&amp;&amp; 

( ISBLANK( Root_Cause__c ) || ISBLANK( Immediate_Action__c) || ISBLANK( Corrective_Action__c ) )</errorConditionFormula>
        <errorMessage>Root Cause, Immediate Action, Corrective Action fields cannot be empty when Activity Type is Implementation.</errorMessage>
    </validationRules>
