    <validationRules>
        <fullName>Validate_Account_Hierarchy</fullName>
        <active>true</active>
        <description>Location Type must be depend upon the Parent Account&apos;s Location Type.</description>
        <errorConditionFormula>NOT(
  OR( 
     AND( ISPICKVAL( Location_Type__c, &apos;HQ / Corporate&apos; ), ( ISBLANK( ParentId ) ) ),
     AND( ISPICKVAL( Location_Type__c, &apos;Division / BU&apos; ), ( ISBLANK( ParentId ) || ISPICKVAL( Parent.Location_Type__c, &apos;HQ / Corporate&apos; ) ) ),
     AND( ISPICKVAL( Location_Type__c, &apos;Sub-Division&apos; ), ( ISBLANK( ParentId ) || ISPICKVAL( Parent.Location_Type__c, &apos;Division / BU&apos; ) ) ),
     AND( ISPICKVAL( Location_Type__c, &apos;Distribution Center&apos; ), (ISBLANK( ParentId ) ) ),
     AND( ISPICKVAL( Location_Type__c, &apos;Shipping Location&apos; ), ( ISBLANK( ParentId ) ) )
))</errorConditionFormula>
        <errorDisplayField>Location_Type__c</errorDisplayField>
        <errorMessage>Please select the appropriate Location Type. You may want to check the Location Type of the Parent Account if available.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Account_Hierarchy_v2</fullName>
        <active>false</active>
        <errorConditionFormula>NOT (OR( 
     AND( ISPICKVAL( Location_Type__c, &apos;HQ / Corporate&apos; ), ( ISBLANK( ParentId ) ) ),
     AND( ISPICKVAL( Location_Type__c, &apos;Distribution Center&apos; ), (ISBLANK( ParentId ) ) ),
     AND( ISPICKVAL( Location_Type__c, &apos;Shipping Location&apos; ), ( ISBLANK( ParentId ) ) )
))</errorConditionFormula>
        <errorMessage>Please select the appropriate Location Type. You may want to check the Location Type of the Parent Account if available.</errorMessage>
    </validationRules>
